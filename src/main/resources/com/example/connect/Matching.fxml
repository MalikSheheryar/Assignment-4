<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" prefHeight="400.0" prefWidth="600.0">
    <VBox spacing="10" layoutX="20" layoutY="20" prefWidth="560">
        <Label text="Matching Results" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
        <TableView fx:id="matchTable" prefHeight="300" prefWidth="560">
            <columns>
                <TableColumn fx:id="nameColumn" text="Name" prefWidth="200" />
                <TableColumn fx:id="compatibilityColumn" text="Compatibility (%)" prefWidth="150" />
                <TableColumn fx:id="messageColumn" text="Message" prefWidth="150" />
            </columns>
        </TableView>
        <Button fx:id="backButton" text="Back" />
    </VBox>
</AnchorPane>
